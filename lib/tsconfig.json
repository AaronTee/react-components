{
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": ".",
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es5",
        "lib": ["es2015", "es6", "es7", "dom"],
        "jsx": "react",
        "allowJs": true,
        "declaration": true,
        "sourceMap": true,
        "importHelpers": true,
        "noImplicitReturns": false,
        "noImplicitThis": false,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "types": ["typePatches"]
}
