{
	"name": "@sebgroup/react-components",
	"version": "0.0.1",
	"description": "A set of React base components based on SEB design",
	"license": "ISC",
	"author": "Mohsen Asle Zaim",
	"keywords": [
		"react",
		"components"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.sebank.se/DesignLibrary/ReactComponents"
	},
	"bugs": {
		"url": "https://github.sebank.se/DesignLibrary/ReactComponents/issues"
	},
	"homepage": "https://github.sebank.se/DesignLibrary/ReactComponents#readme",
	"scripts": {
		"build:dev": "webpack-dev-server --config webpack.config.dev.js --host 0.0.0.0",
		"build:docs": "webpack --config webpack.config.docs.js",
		"build:production": "webpack --config webpack.config.build.js",
		"clean": "rimraf ./dist ./docs",
		"copy": "cp package.json dist/ && cp README.md dist/",
		"lint": "tslint -p tsconfig.json",
		"start": "npm run build:dev",
		"docs": "npm run clean && npm run build:docs",
		"build": "npm run clean && npm run lint && npm run build:production && npm run build:docs",
		"publish": "npm run clean && npm run build",
		"test": "rimraf ./coverage && jest"
	},
	"dependencies": {
		"@sebgroup/bootstrap": "^3.1.1",
		"chart.js": "^2.7.3",
		"chartjs-plugin-annotation": "^0.5.7",
		"cropperjs": "^1.4.3",
		"moment": "^2.24.0",
		"react": "^16.8.3",
		"react-chartjs-2": "^2.7.4",
		"react-date-picker": "^7.2.0",
		"react-dom": "^16.8.3",
		"react-rating": "^1.6.2",
		"react-select": "^2.4.1",
		"react-slick": "^0.23.2",
		"slick-carousel": "^1.8.1"
	},
	"devDependencies": {
		"@types/enzyme": "^3.9.0",
		"@types/jest": "^24.0.9",
		"@types/node": "^11.9.5",
		"@types/react": "^16.8.5",
		"@types/react-dom": "^16.8.2",
		"@types/react-router": "^4.4.4",
		"@types/systemjs": "^0.20.6",
		"babel-polyfill": "^6.26.0",
		"case-sensitive-paths-webpack-plugin": "^2.2.0",
		"copy-webpack-plugin": "^5.0.0",
		"css-loader": "^2.1.0",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.10.0",
		"file-loader": "^3.0.1",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.1.0",
		"jest-babel": "^1.0.1",
		"jest-enzyme": "^7.0.1",
		"jsdom": "^13.2.0",
		"jsdom-global": "^3.0.2",
		"markdown-loader": "^5.0.0",
		"node-sass": "^4.11.0",
		"raf": "^3.4.1",
		"raw-loader": "^1.0.0",
		"react-highlight": "^0.12.0",
		"react-loadable": "^5.5.0",
		"react-router-dom": "^4.3.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"ts-jest": "^24.0.0",
		"ts-loader": "^5.3.3",
		"tslib": "^1.9.3",
		"tslint": "^5.13.0",
		"tslint-react": "^3.6.0",
		"typescript": "^3.3.3333",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	}
}
